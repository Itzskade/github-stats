{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "functions": {
    "api/*.js": {
      "memory": 128,
      "maxDuration": 10
    }
  ],
  "routes": [
    { "src": "/api", "dest": "api/index.js" },
    { "src": "/api/top-langs", "dest": "api/top-langs.js" }
  ]
}
